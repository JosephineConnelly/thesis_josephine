@article{chen:2022aa,
	abstract = {Maize is a globally valuable commodity and one of the most extensively studied genetic model organisms. However, we know surprisingly little about the extent and potential utility of the genetic variation found in wild relatives of maize. Here, we characterize a high-density genomic variation map from 744 genomes encompassing maize and all wild taxa of the genus Zea, identifying over 70 million single-nucleotide polymorphisms. The variation map reveals evidence of selection within taxa displaying novel adaptations. We focus on adaptive alleles in highland teosinte and temperate maize, highlighting the key role of flowering-time-related pathways in their adaptation. To show the utility of variants in these data, we generate mutant alleles for two flowering-time candidate genes. This work provides an extensive sampling of the genetic diversity of Zea, resolving questions on evolution and identifying adaptive variants for direct use in modern breeding.},
	address = {HEIDELBERGER PLATZ 3, BERLIN, 14197, GERMANY},
	author = {Chen, Lu and Luo, Jingyun and Jin, Minliang and Yang, Ning and Liu, Xiangguo and Peng, Yong and Li, Wenqiang and Phillips, Alyssa and Cameron, Brenda and Bernal, Julio S. and Rellan-Alvarez, Ruben and Sawers, Ruairidh J. H. and Liu, Qing and Yin, Yuejia and Ye, Xinnan and Yan, Jiali and Zhang, Qinghua and Zhang, Xiaoting and Wu, Shenshen and Gui, Songtao and Wei, Wenjie and Wang, Yuebin and Luo, Yun and Jiang, Chenglin and Deng, Min and Jin, Min and Jian, Liumei and Yu, Yanhui and Zhang, Maolin and Yang, Xiaohong and Hufford, Matthew B. and Fernie, Alisdair R. and Warburton, Marilyn L. and Ross-Ibarra, Jeffrey and Yan, Jianbing},
	date-added = {2022-11-22 10:16:47 +0100},
	date-modified = {2022-11-22 10:16:47 +0100},
	doi = {10.1038/s41588-022-01184-y},
	issn = {1061-4036},
	journal = {Nature Genet.},
	keywords = {LOCAL ADAPTATION; FLOWERING-TIME; CHROMOSOME INVERSIONS; MOLECULAR EVOLUTION; TEOSINTE ALLELES; READ ALIGNMENT; COMPLEX TRAITS; MAIZE; DOMESTICATION; RESISTANCE},
	month = {October},
	publisher = {NATURE PORTFOLIO},
	times-cited = {0},
	title = {Genome sequencing reveals evidence of adaptive variation in the genus Zea},
	url = {http://ws.isiknowledge.com/cps/openurl/service?url_ver=Z39.88-2004&rft_id=info:ut/WOS:000870632300002},
	wok-uid = {WOS:000870632300002},
