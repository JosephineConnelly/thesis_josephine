	keywords = {MAP-BASED CLONING; CIRCADIAN CLOCK; TIME GENE; PHYTOCHROME-A; GROWTH HABIT; DAY-LENGTH; SEED MASS; LOCUS-T; ARABIDOPSIS; MATURITY},
