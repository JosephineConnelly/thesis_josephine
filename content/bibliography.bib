  times-cited = {37},
  wok-uid = {WOS:000643664100001},
  keywords = {CIRCADIAN CLOCK,FUNCTIONAL EVOLUTION,GLYCINE-MAX,GROWTH HABIT,LONG-JUVENILE TRAIT,MAP-BASED CLONING,MATURITY GENE E1,molecular-designed breeding,photoperiodic flowering,POSITIONAL CLONING,QUANTITATIVE TRAIT LOCUS,SIMPLE SEQUENCE REPEAT,soybean},
