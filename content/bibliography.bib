  author = {Song, Wenwen and Sun, Shi and Ibrahim, Seifeldin Elrayah and Xu, Zejun and Wu, Haiying and Hu, Xingguo and Jia, Hongchang and Cheng, Yanxi and Yang, Zhonglu and Jiang, Shibo and Wu, Tingting and Sinegovskii, Mikhail and Sapey, Enoch and Nepomuceno, Alexandre and Jiang, Bingjun and Hou, Wensheng and Sinegovskaya, Valentina and Wu, Cunxiang and Gai, Junyi and Han, Tianfu},
  year = {2019},
  journal = {Crop Science},
  volume = {59},
  number = {5},
